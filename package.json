{
  "name": "aaf-rapid-connect-mock",
  "version": "0.0.0",
  "description": "Host a 'mock' instance of AAF Rapid Connect.",
  "author": "DSTIL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstil/aaf-rapid-connect-mock.git"
  },
  "bugs": {
    "url": "https://github.com/dstil/aaf-rapid-connect-mock/issues"
  },
  "main": "dist",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist",
    "prebuild": "node_modules/.bin/rimraf dist",
