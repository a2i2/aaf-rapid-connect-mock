    "prepublish": "npm test && npm run build",
    "test": "node_modules/.bin/mocha --compilers js:babel-register"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "jwt-simple": "0.5.0",
    "lodash.template": "4.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-register": "6.16.3",
    "husky": "0.11.9",
    "mocha": "3.1.0",
    "rimraf": "2.5.4",
    "supertest": "2.0.0"
  },
  "keywords": [
    "aaf",
    "rapid",
    "connect",
    "mock",
    "jwt",
    "shibboleth",
    "idp",
    "sp",
    "sso",
    "auth"
  ],
  "babel": {
    "presets": [
      "es2015"
